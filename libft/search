/* ************************************************************************** */

#include "libft.h"

void	ft_int_set(int *array, int set, int len)
{
	int i;
	
	i = 0;
	while (len--)
		array[i++] = set;
}
/* ************************************************************************** */
/*                                                                            */
/*                                                        :::      ::::::::   */
/*   ft_intchr.c                                        :+:      :+:    :+:   */
/*                                                    +:+ +:+         +:+     */
/*   By: jaylor <marvin@42.fr>                      +#+  +:+       +#+        */
/*                                                +#+#+#+#+#+   +#+           */
/*   Created: 2016/12/06 18:26:25 by jaylor            #+#    #+#             */
/*   Updated: 2017/09/07 11:34:28 by jaylor           ###   ########.fr       */
/*                                                                            */
/* ************************************************************************** */

#include "libft.h"

int	ft_intchr(int *array, int match, int len)
{
	int i;
	i = 0;
	
		while (len-- && *array != -1)
		{
			if (array[i] == match)
				return (1);
			i++;
		}
	return (0);
}
/* ************************************************************************** */
/*                                                                            */
/*                                                        :::      ::::::::   */
/*   ft_lstmap.c                                        :+:      :+:    :+:   */
/*                                                    +:+ +:+         +:+     */
/*   By: jaylor <marvin@42.fr>                      +#+  +:+       +#+        */
/*                                                +#+#+#+#+#+   +#+           */
/*   Created: 2016/12/08 14:18:47 by jaylor            #+#    #+#             */
/*   Updated: 2017/11/25 13:56:47 by jaylor           ###   ########.fr       */
/*                                                                            */
/* ************************************************************************** */

#include "includes/libft.h"

int	*ft_intdup(int *array)
{
	int *new;
	int i;
	
	i = 0;
	while (array[i] >= 0)
		i++;
	new = malloc(sizeof(int) * (i + 1));
	ft_intset(new, -1, i + 1);
	i = 0;
	while (array[i] != -1)
	{
	//	printf("i in ft_indup; %d\n", i);
		new[i] = array[i];
		i++;
	}
	return (new);
}
/* ************************************************************************** */
/*                                                                            */
/*                                                        :::      ::::::::   */
/*   ft_intlen.c                                        :+:      :+:    :+:   */
/*                                                    +:+ +:+         +:+     */
/*   By: jaylor <marvin@42.fr>                      +#+  +:+       +#+        */
/*                                                +#+#+#+#+#+   +#+           */
/*   Created: 2016/12/06 14:50:41 by jaylor            #+#    #+#             */
/*   Updated: 2017/03/08 14:37:51 by jaylor           ###   ########.fr       */
/*                                                                            */
/* ************************************************************************** */

#include "libft.h"

int	ft_intlen(intmax_t n)
{
	int i;

	i = 0;
	if (n == 0)
		return (1);
	if (n == -2147483648)
		return (11);
	if (n < 0)
	{
		n *= -1;
	}
	while (n)
	{
		i++;
		n /= 10;
	}
	return (i);
}
/* ************************************************************************** */
/*                                                                            */
/*                                                        :::      ::::::::   */
/*   ft_strrev.c                                        :+:      :+:    :+:   */
/*                                                    +:+ +:+         +:+     */
/*   By: jaylor <marvin@42.fr>                      +#+  +:+       +#+        */
/*                                                +#+#+#+#+#+   +#+           */
/*   Created: 2016/12/06 11:05:52 by jaylor            #+#    #+#             */
/*   Updated: 2017/11/26 13:12:15 by jaylor           ###   ########.fr       */
/*                                                                            */
/* ************************************************************************** */

#include "libft.h"

void    ft_intrev(int *s)
{
    int    temp;
    int    *e;
    int     len;

    len = ft_int_array_len(s);
    e = s + len - 1;
    while (s < e)
    {
        temp = *s;
        *s = *e;
        *e = temp;
        s++;
        e--;
    }
}

/* ************************************************************************** */
/*                                                                            */
/*                                                        :::      ::::::::   */
/*   ft_memset.c                                        :+:      :+:    :+:   */
/*                                                    +:+ +:+         +:+     */
/*   By: jaylor <marvin@42.fr>                      +#+  +:+       +#+        */
/*                                                +#+#+#+#+#+   +#+           */
/*   Created: 2016/11/30 16:24:34 by jaylor            #+#    #+#             */
/*   Updated: 2017/11/25 13:51:02 by jaylor           ###   ########.fr       */
/*                                                                            */
/* ************************************************************************** */

#include "libft.h"

void	*ft_intset(int *b, int c, size_t len)
{
	int	*s;

	s = b;
	while (len > 0)
	{
		*s = c;
		s++;
		len--;
	}
	return (b);
}
