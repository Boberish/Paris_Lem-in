#include "libft.h"

void	ft_put_int_array(int *array)
{
	int i;

	i = 0;
	while (array[i] != -1)
	{
		ft_putnbr(array[i++]);
		ft_putchar('*');
	}
	ft_putchar('\n');
}
/* ************************************************************************** */
/*                                                                            */
/*                                                        :::      ::::::::   */
/*   ft_putchar.c                                       :+:      :+:    :+:   */
/*                                                    +:+ +:+         +:+     */
/*   By: jaylor <marvin@42.fr>                      +#+  +:+       +#+        */
