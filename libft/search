/*                                                        :::      ::::::::   */
/*   ft_abs.c                                           :+:      :+:    :+:   */
/*                                                    +:+ +:+         +:+     */
/*   By: jaylor <marvin@42.fr>                      +#+  +:+       +#+        */
/*                                                +#+#+#+#+#+   +#+           */
/*   Created: 2016/12/06 18:26:25 by jaylor            #+#    #+#             */
/*   Updated: 2017/03/08 14:38:38 by jaylor           ###   ########.fr       */
/*                                                                            */
/* ************************************************************************** */

#include "libft.h"

intmax_t	ft_abs(intmax_t n)
{
	if (n < 0)
		return (n * -1);
	return (n);
}
/* ************************************************************************** */
/*                                                                            */
/*                                                        :::      ::::::::   */
/*   ft_putnbr.c                                        :+:      :+:    :+:   */
/*                                                    +:+ +:+         +:+     */
/*   By: jaylor <marvin@42.fr>                      +#+  +:+       +#+        */
/*                                                +#+#+#+#+#+   +#+           */
/*   Created: 2016/10/27 12:36:20 by jaylor            #+#    #+#             */
/*   Updated: 2017/03/08 13:21:39 by jaylor           ###   ########.fr       */
/*                                                                            */
/* ************************************************************************** */

#include "libft.h"

void	ft_abvpn(int n)
{
	long	nb;

	nb = n;
	if (nb > 9)
	{
		ft_putnbr(nb / 10);
		ft_putnbr(nb % 10);
	}
	else
	{
		ft_putchar(nb + '0');
	}
}
/* ************************************************************************** */
/*                                                                            */
/*                                                        :::      ::::::::   */
/*   ft_allup.c                                         :+:      :+:    :+:   */
/*                                                    +:+ +:+         +:+     */
/*   By: jaylor <marvin@42.fr>                      +#+  +:+       +#+        */
/*                                                +#+#+#+#+#+   +#+           */
/*   Created: 2017/03/14 09:53:37 by jaylor            #+#    #+#             */
/*   Updated: 2017/03/14 10:01:01 by jaylor           ###   ########.fr       */
/*                                                                            */
/* ************************************************************************** */

char	*ft_allup(char *s)
{
	char *start;

	start = s;
	while (*s)
	{
		if (*s >= 97 && *s <= 122)
			*s = (*s - 32);
		s++;
	}
	return (start);
}
/* ************************************************************************** */
/*                                                                            */
/*                                                        :::      ::::::::   */
/*   ft_atoi.c                                          :+:      :+:    :+:   */
/*                                                    +:+ +:+         +:+     */
/*   By: jaylor <marvin@42.fr>                      +#+  +:+       +#+        */
/*                                                +#+#+#+#+#+   +#+           */
/*   Created: 2016/10/31 09:04:12 by jaylor            #+#    #+#             */
/*   Updated: 2016/12/12 09:38:13 by jaylor           ###   ########.fr       */
/*                                                                            */
/* ************************************************************************** */

#include "libft.h"

uintmax_t		ft_atoi(const char *str)
{
	uintmax_t	num;
	int			index;
	int			ifneg;

	index = 0;
	while (str[index] == ' ' || str[index] == '\v' || str[index] == '\t' ||
			str[index] == '\r' || str[index] == '\n' || str[index] == '\f')
		index++;
	num = 0;
	ifneg = 1;
	if (str[index] == '-' || str[index] == '+')
	{
		if (str[index] == '-')
			ifneg = -1;
		index++;
	}
	while (str[index] != '\0' && str[index] >= '0' && str[index] <= '9')
	{
		num = num * 10 + str[index] - '0';
		index++;
	}
	num = num * ifneg;
	return (num);
}
